# platform :ios, '9.0'

target 'SwiftAR' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftAR
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'GoogleSignIn'

  target 'SwiftARTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftARUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
